{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 3
    },
    "phone": {
      "type": "string",
      "minLength": 5,
      "maxLength": 15,
      "unique": true
    },
    "address": {
      "type": "text",
      "minLength": 3
    },
    "zip_code": {
      "type": "string",
      "minLength": 3,
      "maxLength": 8
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
